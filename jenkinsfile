pipeline{
    agent any
    stages{
        stage("Init"){
            steps{
                echo "Init"
                echo "*****************************"
                sh "ls -la"
                echo "Init Completed."
            }
        }
        stage("Build"){
            steps{
                echo "Build"
                echo "*****************************"
                sh "docker ps -a"
                echo "Build Completed."
            }
        }
    }
}